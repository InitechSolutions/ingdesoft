<h1 style="color:#FF8000;">Historial de Precios</h1>
<div class='container'>
  <div class="row" >
    <div class="col-lg-10 col-lg-offset-1 col-md-10 col-md-offset-1">
      <div class="table-responsive">
        <table class="table table-striped table-hover table-success">
        <thead>
          <tr>
            <th>Fecha</th>
            <th>Valor</th>
          </tr>
        </thead>
        <tbody>
          <% @precios.each do |p| %>
            <tr>
              <td><%= p.created_at %></td>
              <td><%= p.valor %></td>
            </tr>
          <% end %>
        </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
<br>

<button type="button" class="btn-lg btn-warning"><i class="glyphicon glyphicon-plus"></i><%= link_to 'Establecer nuevo precio', new_precio_path %></button>

<h1>GANANCIAS</h1>
<div class="container" style="margin-top:20px;">
  <div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1">
    <div class="well" style="border-radius: 35px;">
      <div class="media">
    <%= form_tag(fechas_compras_path, :method => "get") do %>
      <div class="panel-body">
        <strong>Desde:</strong>
        <%= date_select :inicial, start_year: 2025, end_year: 2016%>
      </div>
      <div class="panel-body">
        <strong>Hasta:</strong>
		      <%= date_select :final, start_year: 2025, end_year: 2016%>
      </div>
		  <%= submit_tag 'Buscar', class:"btn btn-link"%>
	<% end %>

</div></div></div></div>
